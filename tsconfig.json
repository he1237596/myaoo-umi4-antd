{
  "extends": "./src/.umi/tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ],
      "@umijs/max": [
        "node_modules/umi"
      ],
      "@umijs/max/typings": [
        "src/.umi/typings"
      ]
      // "antd": [
      //   "node_modules/antd/es/index.d.ts"
      // ]
    }
  }
}
